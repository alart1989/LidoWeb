<style id="mirror-stake-unified">
[data-mirror-faq-panel][data-state="open"]  { display:block !important; height:auto !important; max-height:none !important; overflow:visible !important; opacity:1 !important; }
[data-mirror-faq-panel][data-state="closed"]{ display:none  !important; height:0   !important; max-height:0   !important; overflow:hidden  !important; opacity:0 !important; }
[data-mirror-faq-trigger]{ pointer-events:auto !important; }
</style>
<script>(function(){
  console.log('STAKE UNIFIED FIX v4 ACTIVE');

  var ALLOWED = new Set(['/', '/wrap/', '/unwrap/', '/rewards/', '/withdrawals/request/', '/withdrawals/claim/']);
  var MAIN_BASE = 'http://localhost:8000/';
  var STAKE_ORIGINS = new Set(['https://stake.lido.fi','http://stake.lido.fi']);

  function folderize(p){ if(!p||p==='#')return '/'; return (p!=='/' && !p.endsWith('/')) ? (p + '/') : p; }
  function isMainOriginUrl(u){
    try{ var x = u.startsWith('http') ? new URL(u) : new URL(u, location.origin);
      return x.origin==='https://lido.fi'||x.origin==='https://www.lido.fi'||x.origin==='http://lido.fi'||x.origin==='http://www.lido.fi';
    }catch(e){ return false; }
  }
  function isStakeInternal(u){
    try{ if(!u) return false; if(u.startsWith('/')) return true; var x = new URL(u, location.origin); return STAKE_ORIGINS.has(x.origin)||x.origin===location.origin; }catch(e){ return false; }
  }

  // — переписываем ссылки
  function rewriteLinks(root){
    (root.querySelectorAll?root:document).querySelectorAll('a[href]').forEach(function(a){
      var href = a.getAttribute('href')||'';
      try{
        if(isStakeInternal(href)){
          var u = href.startsWith('/') ? new URL(href, location.origin) : new URL(href);
          var path = folderize(u.pathname);
          if(path==='/withdrawals/') path = '/withdrawals/request/';
          a.setAttribute('href', path);
        } else if (isMainOriginUrl(href)) {
          a.setAttribute('href', MAIN_BASE); // lido.fi → наша главная
        }
      }catch(e){}
    });
  }
  rewriteLinks(document);
  new MutationObserver(function(m){
    m.forEach(function(x){
      if(x.type==='attributes' && x.target.tagName==='A' && x.attributeName==='href') rewriteLinks(x.target);
      if(x.type==='childList'){ x.addedNodes.forEach(function(n){ if(n.nodeType===1) rewriteLinks(n); }); }
    });
  }).observe(document.documentElement,{childList:true,subtree:true,attributes:true,attributeFilter:['href']});

  // ===== FAQ =====
  function markTrigger(el){ if(!el) return; el.setAttribute('data-mirror-faq-trigger',''); el.removeAttribute('aria-disabled'); el.removeAttribute('tabindex'); if(el.style) el.style.pointerEvents=''; }
  function markPanel(p){ if(!p) return; p.setAttribute('data-mirror-faq-panel',''); }

  function findTrigger(path){
    for(var i=0;i<path.length;i++){
      var el = path[i]; if(!el||el.nodeType!==1) continue;
      if(el.tagName==='SUMMARY') return el;
      if(el.getAttribute && el.getAttribute('role')==='button' && el.hasAttribute('aria-controls')) return el;
      if(el.id && /^react-collapsed-toggle-\d+/.test(el.id)) return el;
      if(el.getAttribute && el.getAttribute('data-accordion-trigger')!=null) return el;
    }
    return null;
  }
  function panelFor(trigger){
    if(!trigger) return null;
    var id = trigger.getAttribute && trigger.getAttribute('aria-controls');
    if(id){ var p = document.getElementById(id); if(p){ markPanel(p); return p; } }
    if(trigger.tagName==='SUMMARY'){ var det = trigger.closest('details'); if(det){ markPanel(det); return det; } }
    var sib = trigger.parentElement ? trigger.parentElement.nextElementSibling : null;
    if(sib){ markPanel(sib); return sib; }
    return null;
  }
  function closePanelHard(panel){
    if(!panel) return;
    panel.setAttribute('data-mirror-faq-panel','');
    panel.setAttribute('data-state','closed');
    if(panel.tagName==='DETAILS') panel.open = false;
    panel.style.setProperty('display','none','important');
    panel.style.setProperty('height','0px','important');
    panel.style.setProperty('max-height','0px','important');
    panel.style.setProperty('overflow','hidden','important');
    panel.style.setProperty('opacity','0','important');
    var wrap = panel.parentElement;
    if(wrap && wrap.style){
      wrap.style.setProperty('overflow','visible','important');
      wrap.style.setProperty('height','auto','important');
      wrap.style.setProperty('max-height','none','important');
    }
  }
  function setOpen(trigger, panel, open){
    if(trigger){ trigger.setAttribute('aria-expanded', String(!!open)); trigger.setAttribute('data-state', open ? 'open' : 'closed'); markTrigger(trigger); }
    if(panel){
      panel.setAttribute('data-state', open ? 'open' : 'closed');
      if(panel.tagName==='DETAILS'){ panel.open = !!open; }
      if(open){
        panel.style.setProperty('display','block','important');
        panel.style.setProperty('height','auto','important');
        panel.style.setProperty('max-height','none','important');
        panel.style.setProperty('overflow','visible','important');
        panel.style.setProperty('opacity','1','important');
      } else {
        closePanelHard(panel);
      }
    }
  }
  function forceCloseAll(){
    document.querySelectorAll('[id^="react-collapsed-panel-"], details, [role="region"], [data-accordion-content], [data-faq-panel], [data-mirror-faq-panel]')
      .forEach(closePanelHard);
    document.querySelectorAll('summary,[role="button"][aria-controls],[id^="react-collapsed-toggle-"],[data-accordion-trigger]')
      .forEach(function(t){
        t.setAttribute('data-mirror-faq-trigger','');
        t.removeAttribute('aria-disabled'); t.removeAttribute('tabindex');
        if(t.style) t.style.pointerEvents='';
        t.setAttribute('aria-expanded','false');
        t.setAttribute('data-state','closed');
        var id = t.getAttribute('aria-controls'); if(id){ var p = document.getElementById(id); if(p) closePanelHard(p); }
      });
  }
  function initFaq(){
    forceCloseAll();
    setTimeout(forceCloseAll, 0);
    setTimeout(forceCloseAll, 150);
    setTimeout(forceCloseAll, 600);
  }
  initFaq();

  function onToggle(e){
    var path = e.composedPath ? e.composedPath() : [e.target];
    var t = findTrigger(path);
    if(!t) return;
    var p = panelFor(t);
    var expanded = (t.getAttribute('aria-expanded')==='true');
    setOpen(t, p, !expanded);
    e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation();
  }
  document.addEventListener('click', onToggle, true);
  document.addEventListener('keydown', function(e){ if(e.key==='Enter'||e.key===' ') onToggle(e); }, true);

  // — табы Wrap/Unwrap и Request/Claim → жёсткая навигация
  document.addEventListener('click', function(e){
    var path = e.composedPath ? e.composedPath() : [e.target];
    var el = path.find && path.find(function(x){ return x && x.nodeType===1 && x.textContent; });
    if(!el) return;
    var txt = (el.textContent || '').trim().toLowerCase();
    if (/^unwrap$/.test(txt)) { e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation(); location.href = '/unwrap/'; return; }
    if (/^wrap$/.test(txt))   { e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation(); location.href = '/wrap/';   return; }
    if (/^claim$/.test(txt))   { e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation(); location.href = '/withdrawals/claim/';   return; }
    if (/^request$/.test(txt)) { e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation(); location.href = '/withdrawals/request/'; return; }
  }, true);

 // ===== THEME TOGGLE v3 (надёжно на всех вкладках) =====
  (function(){
    console.log('THEME TOGGLE v3 ACTIVE');
    var LS_KEY = 'lido-theme';
    function apply(t){
      document.documentElement.setAttribute('data-lido-theme', t);
      document.body.classList.toggle('dark', t==='dark');  // страховка
      try{ localStorage.setItem(LS_KEY, t); }catch(_){}
    }
    try{ var saved = localStorage.getItem(LS_KEY); if(saved) apply(saved); }catch(_){}

    function looksLikeToggle(el){
      if(!el || el.nodeType!==1) return false;
      var aria = (el.getAttribute('aria-label')||'').toLowerCase();
      var title = (el.getAttribute('title')||'').toLowerCase();
      if(aria.includes('theme')||aria.includes('mode')) return true;
      if(title.includes('theme')||title.includes('mode')) return true;
      var cls = (el.className||''); if(/theme|mode|dark|light/i.test(cls)) return true;
      // маленькие кнопки-иконки рядом с "Connect wallet"
      if(el.querySelector && el.querySelector('svg') && (el.clientWidth<64||el.clientHeight<64)) return true;
      return false;
    }

    // ищем «родной» тумблер
    var native = document.querySelector(
      '[aria-label*="theme" i], [title*="theme" i], [data-testid*="theme" i], [class*="theme" i], [class*="dark" i], [class*="light" i]'
    );

    // если нет — создаём свой
    var injected = null;
    if(!native){
      injected = document.createElement('button');
      injected.type = 'button';
      Object.assign(injected.style,{
        position:'fixed', right:'16px', top:'16px', zIndex:99997,
        width:'36px', height:'36px', borderRadius:'50%', border:'0',
        background:'rgba(0,0,0,.55)', color:'#fff', cursor:'pointer'
      });
      injected.title='Toggle theme';
      injected.textContent='☾';
      document.body.appendChild(injected);
    }

    var toggleEl = native || injected;

    // обработчик
    function toggle(e){
      e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation();
      var cur = (document.documentElement.getAttribute('data-lido-theme') || 'light').toLowerCase();
      apply(cur==='dark' ? 'light' : 'dark');
    }
    if (toggleEl) toggleEl.addEventListener('click', toggle, true);

    // горячая клавиша: «T» — переключить тему (удобно для проверки)
    document.addEventListener('keydown', function(e){
      if ((e.key||'').toLowerCase()==='t') toggle(e);
    }, true);
  })();


  // — allowlist навигации (после FAQ/табов), плюс абсолютный переход на MAIN_BASE
  function allowlistHandler(e){
    var path = e.composedPath ? e.composedPath() : [e.target];
    var a = path.find && path.find(function(el){ return el && el.tagName==='A' && el.getAttribute; });
    if(!a) return;
    var raw = a.getAttribute('href')||'';
    if (raw && (raw === MAIN_BASE || raw.indexOf(MAIN_BASE) === 0)) {
      e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation();
      location.href = MAIN_BASE; return;
    }
    var href = raw;
    try{ if(href.startsWith('http')) href = new URL(href).pathname; }catch(e){}
    href = folderize(href);
    if(href==='/withdrawals/') href = '/withdrawals/request/';
    if(ALLOWED.has(href)){
      e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation();
      location.href = href; return;
    }
    if(isMainOriginUrl(raw)){
      e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation();
      location.href = MAIN_BASE; return;
    }
    e.preventDefault(); e.stopPropagation(); e.stopImmediatePropagation();
  }
  ['click','pointerdown','mousedown','mouseup','auxclick','touchstart','touchend'].forEach(function(t){
    document.addEventListener(t, allowlistHandler, true);
  });

  var _open = window.open;
  window.open = function(url){
    if(url && (''+url).indexOf('lido.fi')>-1){ location.href = MAIN_BASE; return null; }
    if(url && (''+url).indexOf(MAIN_BASE)===0){ location.href = MAIN_BASE; return null; }
    return null;
  };
})();</script>
</body></html>